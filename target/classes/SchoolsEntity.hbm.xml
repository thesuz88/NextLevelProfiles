<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.nextlevel.spring.Entities.SchoolsEntity" table="schools" schema="nextlevelmvp">
        <id name="schoolId">
            <column name="school_id" sql-type="int(11)"/>
        </id>
        <property name="school">
            <column name="school" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>