<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.nextlevel.spring.Entities.ConferenceEntity" table="conference" schema="nextlevelmvp">
        <id name="conferenceId">
            <column name="conference_id" sql-type="int(11)"/>
        </id>
        <property name="conference">
            <column name="conference" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>