<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.nextlevel.spring.Entities.UsersEntity" table="users" schema="nextlevelmvp">
        <id name="username">
            <column name="username" sql-type="varchar(45)" length="45"/>
        </id>
        <property name="userId">
            <column name="userID" sql-type="int(11)"/>
        </property>
        <property name="firstname">
            <column name="firstname" sql-type="varchar(255)"/>
        </property>
        <property name="lastname">
            <column name="lastname" sql-type="varchar(255)"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(255)"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="city">
            <column name="city" sql-type="varchar(255)"/>
        </property>
        <property name="state">
            <column name="state" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="zipcode">
            <column name="zipcode" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="conference">
            <column name="conference" sql-type="varchar(255)"/>
        </property>
        <property name="school">
            <column name="school" sql-type="varchar(255)"/>
        </property>
        <property name="sport">
            <column name="sport" sql-type="varchar(255)"/>
        </property>
        <property name="position">
            <column name="position" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="gradelevel">
            <column name="gradelevel" sql-type="varchar(255)"/>
        </property>
        <property name="profileImage">
            <column name="profileImage" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="image1">
            <column name="image_1" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="video1">
            <column name="video_1" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>